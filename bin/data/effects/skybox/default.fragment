#version 330 core

// Interpolated values from the vertex shaders
in vec3 UV;
in vec3 Color;
in vec3 Normal;
in vec4 Position;
// Values that stay constant for the whole mesh.
uniform samplerCube Diffuse;


void main()
{

	vec4 blend = texture( Diffuse, UV );
	gl_FragData[0] = blend;
	gl_FragData[1] = vec4(1,0,0,1);
	gl_FragData[2] = Position;
}